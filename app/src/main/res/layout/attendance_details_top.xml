<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView79"
        android:layout_width="0mm"
        android:layout_height="@dimen/mm_55"
        android:background="@color/bg"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/mm_20"
        android:text="统计"
        android:textSize="@dimen/mm_25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.example.smartagriculture.util.CircularProgressView
        android:id="@+id/circularProgressView2"
        android:layout_width="@dimen/mm_100"
        android:layout_height="@dimen/mm_150"
        app:backColor="@color/bg"
        app:backWidth="@dimen/mm_10"
        app:layout_constraintBottom_toBottomOf="@+id/circularProgressView"
        app:layout_constraintEnd_toStartOf="@+id/circularProgressView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/circularProgressView"
        app:progColor="@color/colorAccent"
        app:progWidth="@dimen/mm_10"
        app:progress="65" />

    <com.example.smartagriculture.util.CircularProgressView
        android:id="@+id/circularProgressView"
        android:layout_width="@dimen/mm_100"
        android:layout_height="@dimen/mm_150"
        app:backColor="@color/bg"
        app:backWidth="@dimen/mm_10"
        app:layout_constraintBottom_toBottomOf="@+id/circularProgressView3"
        app:layout_constraintEnd_toStartOf="@+id/circularProgressView3"
        app:layout_constraintStart_toEndOf="@+id/circularProgressView2"
        app:layout_constraintTop_toTopOf="@+id/circularProgressView3"
        app:progColor="@color/colorAccent"
        app:progWidth="@dimen/mm_10"
        app:progress="10" />

    <com.example.smartagriculture.util.CircularProgressView
        android:id="@+id/circularProgressView3"
        android:layout_width="@dimen/mm_100"
        android:layout_height="@dimen/mm_150"
        app:backColor="@color/bg"
        app:backWidth="@dimen/mm_10"
        app:layout_constraintBottom_toBottomOf="@+id/circularProgressView4"
        app:layout_constraintEnd_toStartOf="@+id/circularProgressView4"
        app:layout_constraintStart_toEndOf="@+id/circularProgressView"
        app:layout_constraintTop_toTopOf="@+id/circularProgressView4"
        app:progColor="@color/colorAccent"
        app:progWidth="@dimen/mm_10"
        app:progress="65" />

    <com.example.smartagriculture.util.CircularProgressView
        android:id="@+id/circularProgressView4"
        android:layout_width="@dimen/mm_100"
        android:layout_height="@dimen/mm_150"
        app:backColor="@color/bg"
        app:backWidth="@dimen/mm_10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circularProgressView3"
        app:layout_constraintTop_toBottomOf="@+id/textView79"
        app:progColor="@color/colorAccent"
        app:progWidth="@dimen/mm_10"
        app:progress="65" />

    <TextView
        android:id="@+id/textView80"
        android:layout_width="0mm"
        android:layout_height="@dimen/mm_55"
        android:background="@color/bg"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/mm_20"
        android:text="详情"
        android:textSize="@dimen/mm_25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/circularProgressView4" />
</androidx.constraintlayout.widget.ConstraintLayout>