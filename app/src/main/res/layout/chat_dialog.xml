<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cycle_white"
    android:padding="@dimen/mm_40">

    <TextView
        android:id="@+id/textView135"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/chat_dialog_title"
        android:textColor="@color/title_gray"
        android:textSize="@dimen/mm_23"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView48"
        android:layout_width="@dimen/mm_48"
        android:layout_height="@dimen/mm_48"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/close_blue_iv" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="0mm"
        android:layout_height="@dimen/mm_40"
        android:background="@color/line_color"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/title_color"
        android:textSize="@dimen/mm_23"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView136"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/cycle_orange"
        android:paddingStart="@dimen/mm_60"
        android:paddingTop="@dimen/mm_20"
        android:paddingEnd="@dimen/mm_60"
        android:paddingBottom="@dimen/mm_20"
        android:text="@string/create"
        android:textColor="@color/white"
        android:textSize="@dimen/mm_23"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>